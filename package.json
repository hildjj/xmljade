{
  "name": "xmljade",
  "version": "0.3.4",
  "description": "Transform XML documents with Jade templates",
  "main": "lib/index.js",
  "scripts": {
    "coffee": "coffee -cm -o lib/ src",
    "coverage": "istanbul cover nodeunit test/*.coffee",
    "precoverage": "npm run coffee",
    "test": "nodeunit test/*.coffee",
    "pretest": "npm run coffee",
    "prepublish": "npm run coffee",
    "watch": "watch 'npm run coverage' src/",
    "release": "npm version patch && git push --follow-tags && npm publish",
    "ci": "npm run coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hildjj/xmljade"
  },
  "keywords": [
    "xml",
    "jade",
    "xsl",
    "xslt",
    "transform",
    "template"
  ],
  "author": "Joe Hildebrand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hildjj/xmljade/issues"
  },
  "homepage": "https://github.com/hildjj/xmljade",
  "directories": {
    "lib": "lib"
  },
  "bin": {
    "xmljade": "./bin/xmljade"
  },
  "devDependencies": {
    "coffee-script": "1.10",
    "coveralls": "^2.11.9",
    "istanbul": "0.4",
    "nodeunit": "0.9",
    "source-map-support": "0.4"
  },
  "dependencies": {
    "commander": "2.9",
    "dentin": "0.1",
    "jade": "1.11",
    "jstransformer-cdata": "1.0",
    "libxmljs": "0.17",
    "temp": "0.8",
    "watch": "0.17"
  }
}
